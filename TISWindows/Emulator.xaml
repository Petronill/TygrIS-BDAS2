<Window x:Class="TISWindows.Emulator"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TISWindows"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Emulator" Height="450" Width="978">
    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment ="Top" HorizontalAlignment="Center">
            <Label FontSize="30" FontWeight="Bold" Content="ADMIN KONZOLE"></Label>
            <Label VerticalAlignment="Center" Content="Výběr mezi uživateli a zvířaty"></Label>
            <ComboBox x:Name="userAnimal" Width="137" Height="26"></ComboBox>
            <Button x:Name="btnRemove" Width="137" Height="26" Margin="10 0 0 0 ">Odebrání</Button>
            <Button x:Name="btnAdd" Width="137" Height="26" Margin="10 0 0 0 ">Přidání</Button>
        </StackPanel>
        <StackPanel x:Name="adminWindow" Orientation="Horizontal" Height="408" Margin="30,75,0,0">
           
        </StackPanel>
    </Grid>
</Window>
